\def\ps@myheadings{%
  \let\@oddfoot\@empty\let\@evenfoot\@empty
  \def\@evenhead{\thepage\hfil\leftmark}%
  \def\@oddhead{{\rightmark}
    \ifEnglish
      \footnotesize
      \@Eyears\@Eposteraname \hskip .5zw
      \ifDoctorate         \@Edoctorateabstractname\hfil \@Edoctoratecourse
      \else\ifDoctorateSys \@Edoctorateabstractname\hfil \@Edoctoratesyscourse
      \else\ifDoctorateNet \@Edoctorateabstractname\hfil \@Edoctoratenetcourse
      \else\ifMaster       \@Emasterabstractname   \hfil \@Emastercourse
      \else\ifMasterSys    \@Emasterabstractname   \hfil \@Emastersysdept
      \else\ifMasterNet    \@Emasterabstractname   \hfil \@Emasternetdept
      \else                \@Ebachelorabstractname \hfil \ifScInfo\@EbachelordeptScInfo\else\@Ebachelordept\fi   % Bachelor
      \fi\fi\fi\fi\fi\fi
    \else
      \small
      \@years\@posteraname \hskip .5zw
      \ifDoctorate         \@doctorateabstractname\hfil \@doctoratecourse
      \else\ifDoctorateSys \@doctorateabstractname\hfil \@doctoratesyscourse
      \else\ifDoctorateNet \@doctorateabstractname\hfil \@doctoratenetcourse
      \else\ifMaster       \@masterabstractname   \hfil \@mastercourse
      \else\ifMasterSys    \@masterabstractname   \hfil \@mastersysdept
      \else\ifMasterNet    \@masterabstractname   \hfil \@masternetdept
      \else\ifProject      \@projectabstractname  \hfil \ifScInfo\@bachelordeptScInfo\else\@bachelordept\fi
      \else\ifSeminar      \@seminarabstractname  \hfil \ifScInfo\@bachelordeptScInfo\else\@bachelordept\fi
      \else                \@bachelorabstractname \hfil \ifScInfo\@bachelordeptScInfo\else\@bachelordept\fi     % Bachelor
      \fi\fi\fi\fi\fi\fi\fi\fi
    \fi}%
  \let\@mkboth\@gobbletwo
  \let\sectionmark\@gobble
  \let\subsectionmark\@gobble
}
