\newenvironment{Abstract}{%
  \begin{normalsize}%
    \ifEnglish
      \setlength\parindent{\@Eindent}%
      \renewcommand{\@title}{\@Etitle}%
      \renewcommand{\@author}{\@Eauthor}%
      \renewcommand{\@affiliate}{\@Eaffiliate}%
    \else
      \setlength\parindent{\@Jindent}%
    \fi
    \setlength\baselineskip{14pt}%
    \twocolumn[%
      \begin{center}%
        {\Large\textbf{\@title}} \par
        \vskip 1.5zh
        {\large
          \begin{tabular}{c}%
            \@idnumber\hskip 1.5zw
            \@author\hskip 1.5zw
            【\hskip .5zw\@affiliate\hskip .5zw 】
          \end{tabular}} \par%
        \ifEnglish \else
          \ifDoctorate
